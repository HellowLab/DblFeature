{
  "expo": {
    "name": "DblFeature",
    "slug": "DblFeature",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icons/Android/Icon-512-condensed.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/icons/Android/Icon-512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hellowlab.dblfeature"
    },
    "android": {
      "package": "com.hellowlab.dblfeature",
      "newArchEnabled": true, 
      "usesClearTextTraffic": true 
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "24d8f629-da46-410a-aaf2-5c545ebc7e3a"
      }
    },
    "owner": "hellowlab"
  }
}
